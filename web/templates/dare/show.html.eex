<h2>Show dare</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @dare.name %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @dare.description %>
  </li>

  <li>
    <strong>Goal:</strong>
    <%= @dare.goal %>
  </li>

  <li>
    <strong>Raised:</strong>
    <%= @dare.raised %>
  </li>

  <li>
    <strong>User:</strong>
    <%= @dare.user_id %>
  </li>

</ul>

<%= link "Edit", to: dare_path(@conn, :edit, @dare) %>
<%= link "Back", to: dare_path(@conn, :index) %>
